Void hideSubMenu() {
    declare CMlFrame Menu <=> (Page.GetFirstChild("Menu") as CMlFrame);
    Page.GetClassChildren("sub" , Menu , True);
    foreach (control in Page.GetClassChildren_Result) {
            control.Hide();
    }
}

Text B2T(Boolean val) {
    if (val == True) {
        return "True";
    }
    return "False";
}