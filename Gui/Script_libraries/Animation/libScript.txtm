  declare Real[Integer]   LibManialink_AnimEnd_Rotation for _Control;
  declare Real[Integer]   LibManialink_AnimEnd_Opacity  for _Control;
  declare Vec3[Integer]   LibManialink_AnimEnd_Colorize for _Control;
